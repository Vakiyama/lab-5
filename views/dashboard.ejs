<h1 class="mt-4">Dashboard</h1>
<p class="lead mb-3">Welcome <%= user.name %></p>
<a href="/auth/logout" class="btn btn-secondary">Logout</a>
<% if (sessions) { %>
  <p>Active Sessions:</p>
  <% Object.keys(sessions).forEach(key => { %>
    <p>Session ID: <%= key %><br />
    User ID: <%= sessions[key].passport.user %></p> 
    <form method="post" action="/admin/revoke/<%= key %>">
      <input value="Revoke Session" type="submit" class="btn btn-primary"/>
    </form>
  <% }) %>
<% } %>
